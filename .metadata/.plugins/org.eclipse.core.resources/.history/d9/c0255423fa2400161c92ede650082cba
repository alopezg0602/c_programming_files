#include <stdio.h>
#include <stdlib.h>

int main(void) {

#include <stdio.h>
#include <conio.h>
#include <time.h>

void main() {

	int **ptr = NULL, sum=0, c, d;
	srand (time(NULL));

	ptr = (int *) reserva(5);

	for( c=0; c<5; c++)
	{
		*ptr = (int *) reserva(5);
	}

}

int *reserva(int elementos)
{
    int *ptrAux;
    ptrAux = (int *)malloc(elementos*sizeof(int));
    return ptrAux;
}

	return EXIT_SUCCESS;
}
